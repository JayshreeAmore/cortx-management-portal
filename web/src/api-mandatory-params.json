{
    "/api/v1/alerts": {
        "GET": [
            {
                "paramname": "sortby",
                "datatype": "string",
                "required": false,
                "default": "created_time",
                "validators": "checkSortingParameters"
            },
            {
                "paramname": "dir",
                "datatype": "string",
                "required": false,
                "default": "desc",
                "validators": "checkSortingParameters"
            },
            {
                "paramname": "offset",
                "datatype": "number",
                "required": false,
                "default": 1
            },
            {
                "paramname": "limit",
                "datatype": "number",
                "required": false,
                "default": 10
            }
        ],
        "PATCH": [
            {
                "paramname": "acknowledged",
                "datatype": "boolean",
                "required": false,
                "default": false
            }
        ]
    },
    "/api/v1/sessionkey": {
        "GET": [
            {
                "paramname": "user",
                "datatype": "string",
                "required": true
            }
        ]
    },
    "/api/v1/stats": {
        "GET": [
            {
                "paramname": "from",
                "datatype": "number",
                "required": true
            },
            {
                "paramname": "to",
                "datatype": "number",
                "required": true
            },
            {
                "paramname": "id",
                "datatype": "number",
                "required": false
            },
            {
                "paramname": "interval",
                "datatype": "number",
                "required": false
            },
            {
                "paramname": "metric_list",
                "datatype": "string",
                "required": false,
                "values": "read,write,total"
            },
            {
                "paramname": "format",
                "datatype": "string",
                "required": false
            }
        ]
    },
    "/api/v1/user/admin": {
        "POST": [
            {
                "paramname": "username",
                "datatype": "string",
                "required": true
            },
            {
                "paramname": "password",
                "datatype": "string",
                "required": true
            }
        ]
    },
    "/api/v1/login": {
        "POST": [
            {
                "paramname": "username",
                "datatype": "string",
                "required": true
            },
            {
                "paramname": "password",
                "datatype": "string",
                "required": true
            }
        ]
    }
}